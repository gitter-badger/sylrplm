<h1><%= t("group_new") %></h1>
<% fonct="edit" %>
<% form_for(@group) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @group, :f => f, :fonct=>fonct}) %>
<%= render(:partial => "groups/edit",:locals => { :f => f, :fonct=>fonct} ) %>
<%= render(:partial => "shared/submit_new", :locals => {:object => @group, :f => f, :fonct=>fonct}) %>
<% end %> 