<h1><%= t("forum_item_edit")%></h1>
<% form_for(@forum_item) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "forum_items/edit",:locals => { :f => f, :fonct=>"edit" }) %>
<table class='menu_bas'>
	<tr>
		<td> <%= f.submit t(:update) %> </td>
		<td> <%= link_to t(:explorer), @forum_item %> </td>
		<td> <%= link_to t(:list), forum_items_path %> </td>
	</tr>
</table>
<% end %> 