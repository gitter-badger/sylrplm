<h1><%=t(:sequence_edit) %></h1>
<% fonct="edit" %>
<% form_for(@sequence) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_edit",:locals => { :object => @sequence, :f => f, :fonct=>fonct}) %>
