<h1><%= t(:subscription_new) %></h1>
<% fonct="new" %>
<% form_for @subscription  do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @subscription, :f => f, :fonct=>fonct}) %>
<%= render(:partial => "subscriptions/edit",:locals => { :f => f, :fonct => fonct }) %>
<%= render(:partial => "shared/submit_new", :locals => {:object => @subscription, :f => f, :fonct=>fonct}) %>
<%end %>