<h1><%=t(:project_new) %></h1>
<%fonct="new"%>
<% form_for(@project) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @project, :f => f, :fonct=>fonct}) %>
