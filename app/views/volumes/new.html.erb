<h1><%= t(:volume_new) %></h1>
<% fonct="new" %>
<% form_for(@volume) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @volume, :f => f, :fonct=>fonct}) %>
