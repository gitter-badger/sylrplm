<fieldset>
	<legend>
		<%= t(:legend_typesobject) %>
	</legend>
	<%= h_edit_forobject_object(f, object, @objectswithtype) %>
	<%= f.label t(:label_name) %>
	<%= f.text_field :name %>
	<br/>
	<%= f.label t(:label_description) %>
	<%= h_text_editor f, object, :description%>
	<br/>
	<%= f.label t(:label_typesobject_father) %>
	<%= f.collection_select(:father_id, object.others, :id, :name,  {:include_blank => true}) %>
	<br/>
	<%= f.label t(:label_typesobject_rank) %>
	<%= f.text_field :rank %>
	<br/>
	<%= h_input_domain(f) %>
	<br/>
	<%= f.label t(:label_fields) %>
	<br />
	<%= h_render_fields(object, fonct, "fields") %>
</fieldset>