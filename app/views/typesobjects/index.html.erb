<h1><%= t(:typesobject_list) %><%= h_count_objects(@typesobjects)%> </h1>
<% unless @typesobject.nil? %>
<%= form_for(@typesobject) do |f| %>
<%= form_errors_for @typesobject %>
<% end %>
<% else %>
<%= h_form_simple_query('/typesobjects', @typesobjects) %>
<%= form_tag({:controller=>:typesobjects, :action=>:index_execute}) do %>
<%= h_menu_index_action("typesobject")%>
<%= will_paginate(@typesobjects[:recordset]) %>
<table>
	<tr>
		<th <%= sort_td_class_helper "name" %>> <%= sort_link_helper t(:label_name), "name" %> </th>
		<th <%= sort_td_class_helper "forobject" %>> <%= sort_link_helper t(:label_object), "forobject" %> </th>
		<th <%= sort_td_class_helper "rank" %>> <%= sort_link_helper t(:label_typesobject_rank), "rank" %> </th>
		<th <%= sort_td_class_helper "description" %>> <%= sort_link_helper t(:label_description), "description" %> </th>
		<th> <%= t(:label_typesobject_father) %> </th>
		<th <%= sort_td_class_helper "fields" %>> <%= sort_link_helper t(:label_fields), "fields" %> </th>
		<th <%= sort_td_class_helper "domain" %>> <%= sort_link_helper t(:label_domain), "domain" %> </th>
		<th></th><th></th><th></th>
	</tr>
	<% @typesobjects[:recordset].each do |typesobject| %>
	<tr class='<%= cycle("even", "odd") %>'>
		<td> <%= h_explorer typesobject, :name %> <%= h_show_translate typesobject.name_translate %></td>
		<td><%= h_explorer typesobject, :forobject %><%= h_show_translate typesobject.forobject_translate %></td>
		<td> <%= h typesobject.rank %> </td>
		<td> <%= h typesobject.description %> </td>
		<td> <%= h_explorer typesobject.father, :name unless typesobject.father.nil?%><%= h_show_translate typesobject.father.name_translate unless typesobject.father.nil?%></td>
		<td> <%= h typesobject.fields %> </td>
		<td> <%= h typesobject.domain %> </td>
		<td> <%= link_to h_img_edit.to_s, edit_typesobject_path(typesobject) %> </td>
		<td> <%= h_menu_duplicate(typesobject) %> </td>
		<td> <%= h_destroy(typesobject) %> </td>
	<td><%= check_box(:action_on,typesobject.id) %></td>
	</tr>
	<% end %>
</table>
<%= will_paginate(@typesobjects[:recordset]) %>
<%= h_menu_index_action("typesobject")%>
<%end%>
<%end%>
