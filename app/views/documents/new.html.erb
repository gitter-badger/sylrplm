<h1><%= t("document_new") %></h1>
<% fonct="new"%>
<% form_for(@document) do |f| %>
<%= hidden_field :document , :revision %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_new", :locals => {:object => @document, :f => f, :fonct=>fonct}) %>
<%= render(:partial => "documents/edit",:locals => { :f => f, :fonct=>fonct }) %>
<%= render(:partial => "shared/submit_new", :locals => {:object => @document, :f => f, :fonct=>fonct}) %>

<% end %>