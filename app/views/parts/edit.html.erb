<h1><%= t(:part_edit) %></h1>
<% fonct="edit"%>
<% form_for(@part, :html => { :id => h_form_html_id(@part, fonct) }) do |f| %>
<%= f.error_messages %>
<%= render(:partial => "shared/menu_edit",:locals => { :object => @part, :f => f, :fonct=>fonct}) %>
<%= render(:partial => "parts/edit",:locals => { :f => f, :fonct=>fonct }) %>
<%= render(:partial => "shared/submit_edit",:locals => { :object => @part, :f => f, :fonct=>fonct}) %>
<% end %> 