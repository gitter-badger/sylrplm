<h1><%= t(:datafile_list) %><%= h_count_objects(@datafiles)%> </h1>
<% unless @datafile.nil? %>
<%= form_for(@datafile) do |f| %>
<%= form_errors_for @datafile %>
<% end %>
<% end %>
<%= h_form_simple_query('/datafiles', @datafiles) %>
<table class='menu_toolbar'>
	<tr>
		<td> <%= link_to t("new_datafile"), new_datafile_path %> </td>
		<td> <%= show_help("help_datafiles") %> </td>
	</tr>
</table>
<%= will_paginate(@datafiles[:recordset]) %>
<%= render(:partial => "datafiles/index_datafiles", :locals=>{:datafiles=>@datafiles, :document=>nil, :part=>nil, :project=>nil, :customer=>nil}) %>
<%= will_paginate(@datafiles[:recordset]) %>

<h1><%=t(:sylrplm_files_list)%></h1>
<%= render(:partial => "datafiles/index_sylrplm_files", :locals=>{:files=>@all_files}) %>

