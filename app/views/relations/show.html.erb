<h1><%=t(:relation_show)%></h1>
<%= render(:partial => "shared/menu_show", :locals=>{:object => @relation, :fonct=>"show"}) %>
<% fonct="show" %>
<% form_for(@relation, :html => { :id => h_form_html_id(@relation, fonct) }) do |f| %>
<%= f.error_messages %>
<p>
	<b>Ident:</b>
	<%= h @relation.ident %>
</p>
<p>
	<b>Name:</b>
	<%= h @relation.name %><%=h h_show_translate @relation.name_translate %>
</p>
<p>
	<b>Type:</b>
	<%= h @relation.typesobject.name %><%=h h_show_translate @relation.typesobject.name_translate %>
</p>
<p>
	<b>Father plmtype:</b>
	<%= h @relation.father_plmtype %><%=h h_show_translate @relation.father_plmtype_translate %>
</p>
<p>
	<b>Child plmtype:</b>
	<%= h @relation.child_plmtype %><%=h h_show_translate @relation.child_plmtype_translate %>
</p>
<p>
	<b>Father type:</b>
	<%= h @relation.father_typesobject.name %><%=h h_show_translate @relation.father_typesobject.name_translate %>
</p>
<p>
	<b>Child type:</b>
	<%= h @relation.child_typesobject.name %><%=h h_show_translate @relation.child_typesobject.name_translate %>
</p>
<p>
	<b>Cardin occur min:</b>
	<%= h @relation.cardin_occur_min %>
</p>
<p>
	<b>Cardin occur max:</b>
	<%= h @relation.cardin_occur_max %>
</p>
<p>
	<b>Cardin use min:</b>
	<%= h @relation.cardin_use_min %>
</p>
<p>
	<b>Cardin use max:</b>
	<%= h @relation.cardin_use_max %>
</p>
<%= h_type_values(@relation, fonct) %>
<p>
	<b><%= t("label_domain") %></b>
	<%= h @relation.domain %>
</p>
<% end %>

